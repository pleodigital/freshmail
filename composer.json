{
    "name": "pleodigital/freshmail",
    "description": "Connect your freshmail account to Craft CMS.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "freshmail"
    ],
    "support": {
        "docs": "https://github.com/pleodigital/freshmail/blob/master/README.md",
        "issues": "https://github.com/pleodigital/freshmail/issues"
    },
    "license": "MIT",
    "authors": [{
        "name": "Pleo Digital",
        "homepage": "https://pleodigital.com/"
    }],
    "require": {
        "craftcms/cms": "^3.0.0-RC11|^4.0|^5.0",
        "php": "^7.2.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "pleodigitalfreshmail\\freshmail\\": "src/"
        }
    },
    "extra": {
        "name": "Freshmail",
        "handle": "freshmail",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.pleodigital.com/pleodigital/freshmail/master/CHANGELOG.md",
        "components": {
            "freshmail": "pleodigitalfreshmail\\freshmail\\services\\Freshmail"
        },
        "class": "pleodigitalfreshmail\\freshmail\\Freshmail"
    }
}
